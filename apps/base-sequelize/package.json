{
    "name": "astro-test",
    "version": "0.0.1",
    "scripts": {
        "dev": "astro dev",
        "build": "astro build",
        "preview": "astro preview",
        "astro": "astro",
        "serve": "nest start --watch --debug  --path tsconfig.server.json",
        "build:serve": "nest build --path tsconfig.server.json",
        "build:client": "astro build",
        "db:generate": "pnpx ts-node ./node_modules/drizzle-kit/bin.cjs generate",
        "db:migrate": "pnpx ts-node ./node_modules/drizzle-kit/bin.cjs migrate",
        "db:seed": "ts-node -r tsconfig-paths/register  --project tsconfig.server.json src/infrastructure/providers/database/seeder/run-seed.ts",
        "db:push": "pnpx ts-node ./node_modules/drizzle-kit/bin.cjs push",
        "db:studio": "pnpx drizzle-kit studio",
        "biome:format": "pnpm exec biome format --write",
        "biome:lint": "pnpm exec biome lint --write",
        "biome:check": "pnpm exec biome check --write",
        "biome": "pnpm run biome:format && pnpm run biome:lint && pnpm run biome:check",
        "test": "vitest --config ./astro.config.mjs",
        "test:watch": "vitest --watch  --config ./astro.config.mjs",
        "test:cov": "vitest --coverage  --config ./astro.config.mjs",
        "test:e2e": "vitest --config ./vitest.config.ts"
    },
