{
    "name": "template-app",
    "version": "0.0.1",
    "description": "Stateless NestJS Template with SWC and Biome",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "biome format --write .",
        "start": "nest start",
        "start:dev": "node --max-old-space-size=4096 node_modules/@nestjs/cli/bin/nest.js start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "biome check --apply .",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:cov": "vitest run --coverage",
        "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
        "test:e2e": "vitest run --config ./test/vitest-e2e.config.ts"
    },
    "dependencies": {
        "@nestjs/axios": "^4.0.1",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.0.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/jwt": "^11.0.1",
        "@nestjs/mapped-types": "^2.1.0",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/serve-static": "^5.0.4",
        "@nestjs/swagger": "^11.2.3",
        "@nestjs/terminus": "^11.0.0",
        "@nestjs/throttler": "^6.5.0",
        "@prisma/client": "^7.0.1",
        "@tus/file-store": "^2.0.0",
        "@tus/server": "^2.3.0",
        "@types/formidable": "^3.4.6",
        "@types/passport-google-oauth20": "^2.0.17",
        "@vigilio/valibot": "^0.3.2",
        "axios": "^1.13.2",
        "bcrypt": "^6.0.0",
        "express": "^5.2.1",
        "ffmpeg-static": "^5.3.0",
        "fluent-ffmpeg": "^2.1.3",
        "formidable": "^3.5.4",
        "helmet": "^8.1.0",
        "ioredis": "^5.8.2",
        "minio": "^8.0.6",
        "nestjs-pino": "^4.5.0",
        "passport": "^0.7.0",
        "passport-google-oauth20": "^2.0.0",
        "passport-jwt": "^4.0.1",
        "pino-http": "^11.0.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1",
        "sharp": "^0.34.5"
    },
    "devDependencies": {
