        "@types/bcrypt": "^6.0.0",
        "@types/express": "^4.17.17",
        "@types/fluent-ffmpeg": "^2.1.28",
        "@types/multer": "^2.0.0",
        "@types/node": "^20.3.1",
        "@types/passport-jwt": "^4.0.1",
        "@types/supertest": "^2.0.12",
        "@vitest/coverage-v8": "^4.0.15",
        "prisma": "^7.0.1",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
